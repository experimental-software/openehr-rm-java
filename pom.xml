    <!-- <module>openehr-rm-ehr-extract</module> -->
    <!-- <module>openehr-rm-integration</module> -->
    <!-- <module>openehr-rm-support</module> -->
  </modules>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
