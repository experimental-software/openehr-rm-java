  <groupId>io.github.openehr4j.rm</groupId>
  <artifactId>openehr4j-rm</artifactId>
  <version>1.1.0-alpha-20-SNAPSHOT</version>
  <packaging>pom</packaging>
  <properties>
    <openehr.base.version>1.2.0-alpha-11</openehr.base.version>
